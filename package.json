    "@typescript-eslint/parser": "^6.4.0",
    "autoprefixer": "10.4.15",
    "eslint": "8.47.0",
    "eslint-config-next": "13.4.18",
    "husky": "^8.0.3",
    "jest-coverage-badges": "^1.1.2",
    "next": "13.4.18",
    "postcss": "8.4.28",
    "prettier": "^3.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
